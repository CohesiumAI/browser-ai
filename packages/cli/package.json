{
  "name": "@browser-ai/cli",
  "version": "1.0.0",
  "description": "CLI tools for browser-ai",
  "type": "module",
  "bin": {
    "browser-ai": "./dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "test": "vitest run --passWithNoTests"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.0",
    "typescript": "^5.6.3",
    "tsup": "^8.2.4",
    "vitest": "^2.1.9"
  },
  "keywords": [
    "browser-ai",
    "cli",
    "worker",
    "eject"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
